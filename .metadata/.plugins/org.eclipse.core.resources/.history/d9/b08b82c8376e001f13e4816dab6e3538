
public class Main {

	public static void main(String[] args) {

		
		int[] vecty;
		
		vecty = new int[5];
		
		System.out.println(vecty);
		
		
		int longitud = vecty.length;
		
		System.out.println(longitud);
		
		vecty[1] = 5;
		
		int indice = 2;

		System.out.println(vecty[indice]);
		System.out.println(vecty[3]);
		
		
		

	}

}
